@prefix in: <http://n3.w3c.org/builtin/input#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix cond: <http://rdf.org/cond#> .
@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix state: <http://rdf.org/state#> .
@prefix cm: <http://rdf.org/cmmn#> .

cm:task1 a cm:PlanItem .
cm:task1 state:in ( state:Inactive 0 ) .
# cm:task1 state:in ( state:Ready 1 ) .
# cm:task1 state:in ( state:Active 2 ) .

{
    ?planItem a cm:PlanItem . # tasks, stages, milestones
    _:t log:notIncludes {
        ?stage cm:hasChild ?planItem
    } .

    ?planItem state:curState ( state:Inactive ?state ) .
    ?state state:transit ( state:Ready ?nextState ).

} => {
    ?planItem state:in ?nextState
} .